    margin-left: 5px;
}

#top-menu ul li {
    float: left;
    margin: 0 0 0 15px;
    color: #FFF;
}

#top-menu ul li a {
    color: #FFF !important;
}

#top-menu-bg-repeat {
    position: absolute;
    background: url(/images/top-menu-bg-repeat.png) top right repeat-x;
    height: 43px;
    width: 100%;
    right: 0;
    top: 0;
}

