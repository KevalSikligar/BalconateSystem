/* --- ADMIN PANEL --- */
/*-----------------------------*/
/**/
#adminpanel {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    height: 30px;
    background-color: #7abeff;
    border-top: 2px dashed #383838;
    padding: 0 0 0 10px;
    z-index: 10;
}

