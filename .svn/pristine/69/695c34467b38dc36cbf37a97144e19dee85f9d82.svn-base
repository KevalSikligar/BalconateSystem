/* Top menu */
#top-menu {
    height: 38px;
    /*background: url(/images/top-menu-bg.png) no-repeat top left #FFF;*/
    width: 50%;
    min-width: 500px;
    text-transform: uppercase;
    position: absolute;
    right: 0;
    background-color: #C7C7C7;
    padding-right: 50px;
    border-left: #262626;
    border-bottom: #262626;
    padding-top: 2px;
    -webkit-box-shadow: rgba(83, 83, 83, 0.4) 3px 3px 3px !important;
    -moz-box-shadow: rgba(83, 83, 83, 0.4) 3px 3px 3px !important;
    box-shadow: rgba(83, 83, 83, 0.4) 3px 3px 3px !important;
    border-bottom-left-radius: 5px;
    -webkit-bottom-right-border-radius: 5px;
    -moz-border-radius-bottomleft: 5px;
}

#top-menu ul {
    margin-top: 2px;
    line-height: 35px;
}

#top-menu ul.social {
    float: left;
    margin-left: 5px;
}

