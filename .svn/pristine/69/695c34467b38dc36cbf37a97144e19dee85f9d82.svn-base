/* Articles menu */
#articles-menu {
    float: left;
    width: 100%;
    margin: 0 0 5px 0;
    border-bottom: 1px dotted #666;
    padding-bottom: 20px;
    position: relative;
}

#articles-menu #b1 {
    float: left;
    width: 90px;
    text-align: center;
}

#articles-menu #b1 img {
    display: none;
}

#articles-menu #b1 h3 {
    font-weight: bold;
    font-size: 13px;
    margin-bottom: 20px;
}

#articles-menu .col {
    float: left;
    margin-left: 40px;
}

#articles-menu .col.last {
    margin-right: 0;
}

#articles-menu input, #articles-menu label {
    vertical-align: middle;
}

.article {
    display: inline-block;
    width: 300px;
    padding: 10px;
    margin: 0 16px 20px 0;

    border-radius: 2px;
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
    border: 1px solid #c1c1c1;

    -webkit-box-shadow: 0px 0px 20px 0px rgba(50, 50, 50, 0.5);
    -moz-box-shadow: 0px 0px 20px 0px rgba(50, 50, 50, 0.5);
    box-shadow: 0px 0px 20px 0px rgba(50, 50, 50, 0.5);

}

.article .title {
    display: block;
    text-align: left;
    height: 17px;
    font-weight: bold;
    overflow: hidden;
    margin-bottom: 5px;
    font-size: 13px;
}

.article .img {
    display: inline-block;
    position: relative;
    margin-bottom: 5px;
}

.article .img img {
    height: 130px;
}

.article .img span {
    position: absolute;
    bottom: 0;
    left: 0;
    background: white;
    padding: 4px;
    font-weight: bold;
    opacity: 0.8;
}

.article .desc {
    overflow: hidden;
    text-align: left;
    height: 44px;
    font-size: 12px;
}


