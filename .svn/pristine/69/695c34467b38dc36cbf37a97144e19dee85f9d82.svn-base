/*OWL CARUSEL*/
.carousel-wrap {
    
    padding: 0 5%;
    width: 80%;
    position: relative;
}

/* fix blank or flashing items on carousel */
.owl-carousel .item {
    position: relative;
    z-index: 100;
    -webkit-backface-visibility: hidden;
}

/* end fix */
.owl-nav > div {
    margin-top: -26px;
    position: absolute;
    top: 50%;
    color: #cdcbcd;
}

.owl-nav i {
    font-size: 40px;
}

.owl-nav .owl-prev {
    left: -30px;
}

.owl-nav .owl-next {
    right: -30px;
}
.owl-nav .owl-prev, .owl-nav .owl-next {
    position: absolute;
    top: 50px;
}
    .owl-nav .owl-prev:focus, .owl-nav .owl-next:focus {
        outline:none;
    }
@media only screen and (min-width: 768px) and (max-width: 1024px) {
    .owl-nav .owl-prev, .owl-nav .owl-next {
        position: absolute;
        top: 75px;
    }
    .fancybox-nav span {
        visibility: visible !important;
    }
    .fancybox-wrap {
        left: 12% !important;
    }
}
    @media only screen and (min-width: 1024px) {
        .owl-nav .owl-prev, .owl-nav .owl-next {
            position: absolute;
            top: 60px;
        }

        .owl-nav i {
            font-size: 55px;
        }
    }

    .fa-caret-left .fa-caret-right {
        color: #C0C0C0;
    }
