      <appender-ref ref="GeneralLog" />
    </root>
    <logger name="NHibernate">
      <level value="ERROR" />
    </logger>
    <logger name="NHibernate.SQL">
      <level value="ERROR" />
    </logger>
    <logger additivity="false" name="NotFoundErrors">
      <level value="DEBUG" />
      <appender-ref ref="NotFoundErrorsAppender" />
    </logger>
  </log4net>
