/* general */
body{
border-top: 5px solid #CC0000;
	margin:0px auto;
	font-size: 12px;
	font-family: Verdana, Arial, Helvetica, Sans-Serif;
	color:#000000;
	background: #fff;
	
	
}

img {
	border:0px;
	padding:5px;
	max-width: 100%; /* not working in IE6*/
}
a {color:#b81214;text-decoration: underline;}
a:hover {color: #ff0000;text-decoration: none;}

h1 {
  margin: 0;
	padding: 0;
  font-size:24px; 
  line-height:1.6em;

}
h2 {font-size:20px;}
h3 {font-size:17px;}
h4 {font-size:15px;}
h5 {font-size:14px;}
h6 {font-size:12px;}

p {margin: 0px 0px 12px 0px;}
hr {height:1px;border:none;border-top:1px dashed #b81214; width:90%;}

blockquote {
	clear: left;
	text-align: justify;
	padding: 0px 0px 0px 22px;
	margin: 10px 10px 10px 10px;
	background: url(images/quote.gif) no-repeat top left;
}
input {
	margin-bottom:5px;
	vertical-align: middle;
}
select{
	font-family: Verdana, Arial, Helvetica, Sans-Serif;
}
code{
	font: 1.1em 'Courier New', Courier, Fixed;
}
.alignleft {float: left;}
.alignright {float: right;}
.aligncenter,div.aligncenter{
   display: block;
   margin-left: auto;
   margin-right: auto;
}
img.alignleft {
	padding: 5px;
	margin: 0 7px 2px 0;
	display: inline;
}

img.alignright {
	padding: 5px;
	margin: 0 0 2px 7px;
	display: inline;
}
img.centered {
	display: block;
	margin-left: auto;
	margin-right: auto;
}
.clear {clear:both;}
.spacer {height:20px;}
/***** layout *****/
#page_top{
	margin:0px auto;
	padding:0;
	width:100%;
	background: #fff;
	border:0px blue solid;
}

#base{
	margin:0 auto;
	padding:0;
	width:987px;
	background: #ffffff;
	padding:0 28px 0 28px;
	border:0px red solid;
}

#header{
	position:relative;
	width: 100%;
	height: 117px;
	background:#fff;
}
#menu{
	position:relative;
	width: 100%;
	height: 168px;
	background:#fff;
	padding:0px 0px 0px 0px;

}
#container{
	position:relative;
	overflow:hidden;
	width:987px;
	background: url(http://www.balconette.co.uk//blog/wp-content/themes/3d-realty/images/blogtopbanner7514.jpg) top left no-repeat;
}
#sidebar{
	float:left;
	width:220px;
	overflow:hidden;
	color:#b81214;
	padding:240px 0px 70px 0px;
	border:0px red solid;
}
#content{
	float:right;
	display:inline; 
	width:705px;
	overflow:hidden; 
	padding: 180px 15px 0 10px;
	border:0px red solid;
}
#footer{
	clear:both;
	position:relative;	
	height:150px;
	color:#b81214;
	font-size:11px;
	font-family:Verdana, Arial;
	padding: 30px 5px 5px 50px;
	background:#fff;
}

#footer a {
	display: inline;
	font-size: 11px;
	font-family:Verdana,Arial;
	font-weight: none;
	color:#b81214;
}

#footer a:hover{
	color:#ff0000;
	text-decoration:underline;
}
/*######### header ############*/
#blogtitle {
	position:absolute; 
	top:8px;
	padding-left:3px;
	width:987px;
	font-family: Arial,Verdana; 
	font-size:30px; 
	font-weight:bold;
	text-align:left;
	color:#b81214;
	border:0px red solid;
	overflow:hidden;
	
}

#blogtitle a, #blogtitle a:hover {
	font-family: Arial, Verdana; 
	font-weight:bold;
	color:#b81214; 
	text-decoration:none;
}
	
#subtitle {
	position:absolute;
	overflow:hidden; 
	font-family: Tahoma,Arial, Verdana; 
	width:987px; 
	height:45px;
	top:40px; 
	text-align:left; 
	font-size:13px;
	letter-spacing: 2px;
	color: #ff0000; 
	font-style:italic;
	border:0px red solid;
	line-height:1.1em;
	padding-left:3px;
}
#headtop {
	background:url('images/headtopimg.jpg');
  position:absolute;
	overflow:hidden; 
	font-family: Tahoma,Arial, Verdana; 
	width:987px; 
	height:117px;	
	text-align:left; 
	font-size:13px;
	letter-spacing: 2px;
	color: #ff0000; 
	font-style:italic;
	border:0px red solid;
	line-height:1.1em;
	padding-left:3px;
}


#menu ul {
	position:absolute;
	top: 0px;
	left: 22px;
	height: 168px;
	width: 887px;
	overflow:hidden;
	margin:0px;
	padding:0px;
	padding-top: 0px;
	background:url(images/homebg-2.jpg) no-repeat center;
	
	
}
#menu ul li {
	display: inline;
	font-size: 13px;
	font-family:Verdana,Arial;
	font-weight: bold;
	color:#000000;
}
#menu ul li.separator {
	display: block;
	float: left;
	width:3px;
	height: 34px;
	background:url(images/menu_separate.jpg) no-repeat center;
}

#menu ul li a {
	display: block;
	float: left;
	line-height: 34px;
	color: #ff0000;
	padding: 0px 10px 0px 10px;
	margin:0px 0 0 0 ;
	text-decoration:none;
}

#menu ul li a:hover, #menu ul li.current_page_item a {
	color:#b81214;
}
.searchbox {
	position:absolute;
	top:1px; 
	right: 45px;
	width:250px;
	height:30px; 
	border:0px;
}
.search-field input {
	position:absolute;
	top:5px;
	left:7px;
	width:172px; 
	height:15px; 
	font-family:Verdana;
	font-size:11px; 
	font-weight:bold;
	color:#999999; 
	padding:5px 3px 3px 3px;
	border:0px;
	background:#ffffff;
}
.search-but input {
	position:absolute;
	top:7px;
	right:5px;
	width:50px; 
	height:20px; 
	background:transparent;
	border:0px;
}
#footer img.rss{
	position: absolute;
	right: 52px;
	top: 2px;
	height:75px;
	width:70px;
}
.3dflash { 
width:100%;
height: 312px;
}
.flash-swf {      
height:312;
width:100%;
}
/***** sidebar *****/
#sidebar a{color:#b81214;text-decoration:underline;}
#sidebar a:hover {color:#ff0000;text-decoration:none;}
#sidebar ul {
	list-style-type: none;
	margin: 0px 0px 0px 0px;
	padding: 0px 0px 0px 0px;
	font-family:Verdana,Arial;
}
#sidebar li {
	list-style-type: none;
	margin: 0px 0px 0px 0px;
	padding: 0px 0px 0px 0px;
	font-family:Verdana,Arial;
	
	
}
#sidebar ul ul li{
	position: relative;
	line-height: 1.7em;
	list-style-type: none;
	font-size:12px;
	border:0px red solid;
	font-family:Verdana,Arial;
	margin: 0px 0px 0px 5px;
	padding:0 5px 0 16px;
	background: url('images/sidebar_arrow.jpg') no-repeat 0px 4px;
}

#sidebar ul ul ul li {
	position: relative;
	list-style-type: none;
	font-size:12px;
	border:0px; 
	margin: 0px 0px 0px 0px; 
	line-height: 1.7em; 
	font-family:Verdana,Arial;
	background: url('images/sidebar_arrow.jpg') no-repeat 0px 4px;
	padding: 0px 0px 0px 16px;
}

#sidebar h1 {
	color: #fff;
	font-size: 14px;
	font-weight: bold;
	letter-spacing: 1px;
	margin: 10px 0px 0px 0px;
	padding: 12px 0px 0px 22px;
	height: 30px;
	font-family:Verdana,Arial;
	background:url('images/sidebar_title.jpg') no-repeat center;
	border:0px red solid;
}

#sidebar h2 {
	color: #fff;
	font-size: 14px;
	font-weight: bold;
	letter-spacing: 1px;
	margin: 10px 0px 0px 0px;
	padding: 12px 0px 0px 22px;
	height: 30px;
	font-family:Verdana,Arial;
	background:url('images/sidebar_title.jpg') no-repeat center;
	border:0px red solid;
}

#sidebar select{
	margin:5px 0px 2px 5px;
	font-size:12px;
	width:200px;
	overflow:hidden;
}

/***** content *****/

.archivetitle {
	height:20px;
	color: #666;
	font-size: 28px;
	font-weight:normal;
	padding: 0 90px 30px 0;
	margin: 0px 0px 0px 0px;
  text-align:center; 
}
.post{
	width:605px;
	margin:0 0 20px 0;
	border:0px red solid;
}	

.post_top{
	position: relative;
	border-right: 3px #ff0000 solid;
	border-bottom: 1px #ff0000 dotted;
	height:70px;
	background-color: #f1f2ef;
	margin-bottom:10px;
	margin-top:30px;
	/*background: #f6f6f6;*/
}
.post_bottom{
	margin: 0 50px 0 50px;
	height:3px;
	background:url(images/line.jpg) repeat-x bottom;
}	
.posttitle{
	position:absolute;
	top:0px;
	left:5px;
	font-size:18px;
	font-family:arial;
	color:#b81214;
	border:0px red solid;
	margin-bottom:10px;

}
.posttitle a{
	font-family:arial;
	color:#b81214;
	text-decoration:none;
	margin-bottom:10px;
}
.posttitle a:hover{
	font-family:arial;
	text-decoration:underline;
	color:#ff0000;
	margin-bottom:10px;
}

.posttitle h1{
	font-family:arial;
	color:#b81214;
	text-decoration:none;
	margin-bottom:10px;
  line-height: 42px;
}

.posttitle h1:hover{
	font-family:arial;
	text-decoration:underline;
	color:#ff0000;
	margin-bottom:10px;
	line-height: 42px;
}
.date {
	position:absolute;
	font-family:arial;
	top:44px;
	right: 20px;
	font-size: 10px;
	padding: 0px 0px 0px 0px;
}
.author{
	position:absolute;
	font-family:arial;
	font-size:11px;
	top:44px;
	left:5px;
}
.author a{
	text-decoration:none;
}
.author a:hover{
		text-decoration:underline;
}
.info {
	line-height: 2em;
	font-size: 9px;
	padding:10px 10px 5px 10px;
	color:#000000;
	border:0px red solid;
}
.category{
	padding:2px 0 2px 20px;
	background:url(images/category.jpg) no-repeat top left;
}
.bubble{
	padding:2px 0 2px 20px;
	background:url(images/bubble.jpg) no-repeat left top;
}
.tags{
	padding:2px 0 2px 20px;
	background:url(images/tags.jpg) no-repeat left top;
}

.entry {
	color: #000000;
	line-height: 1.6em;
	text-align: justify;
	padding: 5px 15px 0px 15px;
}

.navigation {
	font-size: 12px;
	padding: 30px 0px 30px 0px;
}


.sorry {
	font-size: 11px;
	font-style: italic;
	margin: 8px 0px 0px 0px;
	padding: 10px 10px 10px 10px;
	border: 1px dashed #ccc;
	background:#ffffff;
}

/*** comment ***/
